<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" 
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.bob.bless.timer</string>

  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>/Users/G/BOB/000/bless_BOB.sh</string>
  </array>

  <key>StartInterval</key>
  <integer>99</integer> <!-- every 99 sec -->

  <key>RunAtLoad</key>
  <true/>

  <key>StandardOutPath</key>
  <string>/tmp/bless_BOB.log</string>

  <key>StandardErrorPath</key>
  <string>/tmp/bless_BOB.err</string>
</dict>
</plist>
